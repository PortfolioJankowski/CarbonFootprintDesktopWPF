<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:CarbonFootprintDesktopApp.ViewModel"
                    xmlns:view ="clr-namespace:CarbonFootprintDesktopApp.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ScopesVM}">
        <view:Scope/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CategoriesVM}">
        <view:Categories/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SourcesVM}">
        <view:Sources/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LocationsVM}">
        <view:Locations/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FactorsVM}">
        <view:Factors/>
    </DataTemplate>


</ResourceDictionary>